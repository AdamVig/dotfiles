[time]
command=echo "<span font_weight=\"bold\">$(date '+%l:%M %p')</span>"
interval=1
markup=pango

[date]
command=date '+%Y-%m-%d'
interval=5

[time eastern]
command=TZ='America/New_York' date '+%l %p %Z'
interval=1

[time pacific]
command=TZ='America/Los_Angeles' date '+%l %p %Z'
interval=1

[audio]
command=SPEAKERS_SINK=alsa_output.pci-0000_00_1f.3.analog-stereo HEADPHONES_SINK=alsa_output.usb-MOTU_M2_M20000044221-00.analog-stereo status-pulseaudio
interval=persist
markup=pango
